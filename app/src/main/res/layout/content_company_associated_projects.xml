<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/content_company_associated_projects"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/lecetModifyToolbarBlue"
        app:layout_behavior="@string/appbar_scrolling_view_behavior"
        tools:showIn="@layout/activity_company_associated_projects"
        tools:context="com.lecet.app.content.CompanyAssociatedProjectsActivity">

    <android.support.v7.widget.RecyclerView
            android:id="@+id/tracking_list_recycler_view"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            tools:listitem="@layout/list_item_mcd_project"/>


</RelativeLayout>
